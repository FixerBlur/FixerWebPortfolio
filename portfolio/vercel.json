{
  "builds": [{
      "src": "portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
  },
  {
      "src": "static/**",
      "use": "@vercel/static"
  },
  {
      "src": "media/**",
      "use": "@vercel/static"
  }],
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/media/(.*)",
          "dest": "/media/$1"
      },
      {
          "src": "/(.*)",
          "dest": "portfolio/wsgi.py"
      }
  ]
}